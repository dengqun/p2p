<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="StateMapper">
   
    
    <insert id="insertBor" parameterType="java.util.Map" >
    insert into p2p_state (stateNo,uid,stateDate,borId,action)
    values(#{stateNo},#{uid},#{stateDate},#{borId},#{action})
    </insert>
    
    
</mapper>